{% extends "@Test/demo.html.twig" %}

{% block body %}
    <div class="demo--Field--field">
        {% for testCase in testCases %}
            <div class="test-case">
                <div class="test-case-title">
                    {{ testCase.title }}
                </div>
                <div class="test-case-content">
                    {% include "@Src/Field/Formatter/image-formatter.html.twig" with testCase.data %}
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}
