{% set safeItems = [] %}

{% for item in items %}
    {% if (item.attributes is not defined) %}
        {% set item = item|merge({
            attributes: drupal_attribute()
        }) %}
    {% endif %}

    {% set safeItems = safeItems|merge([item]) %}
{% endfor %}

{% include "../../../src/Field/Formatter/image-formatter/index.html.twig" with {
    attributes: (attributes is not defined) ? drupal_attribute() : attributes,
    title_attributes: (title_attributes is not defined) ? drupal_attribute() : title_attributes,
    items: safeItems
} %}
